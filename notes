uint32_t
fid(char *path)
{
	PFid	*f;

	f = lookup(path);
	if(f->fid == 0)
		f->fid = p9walk(path);
	return f->fid;
}

PFid*
lookup(char *path)
{
	FFid	*f;
	int	siz, h;

	h = hash(path);
	for(f = fidhash[h]; f != NULL; f = f->link){
		if(strcmp(f->path, path) == 0)
			break;
	}
	if(f == NULL){
		f = emalloc(sizeof(*f));
		siz = strlen(path) + 1;
		f->path = emalloc(siz);
		strecpy(f->path, f->path + siz, path);
		f->link = fidhash[h];
		fidhash[h] = f;
	}
	return f;
}

